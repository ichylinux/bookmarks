gem 'turbolinks'
gem 'uglifier', '>= 1.3.0'

group :development do
	gem 'spring'
end

group :development, :test do
  gem 'thin'
end

group :test do
  gem 'minitest'
  gem 'minitest-reporters'
end

# Use unicorn as the app server
gem 'unicorn'

# Use Capistrano for deployment
group :development do
  gem 'capistrano'
  gem 'capistrano-bundler'
  gem 'capistrano-rails'
end

gem 'sdoc', '~> 0.4.0',          group: :doc

