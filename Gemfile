gem 'coffee-rails', '~> 4.0.0'
gem 'compass', '0.12.7'
gem 'compass-rails'
gem 'daddy'
gem 'devise'
gem 'feedjira'
gem 'jbuilder', '~> 2.0'
gem 'jquery-rails'
gem 'jquery-ui-rails'
gem 'mysql2'
gem 'nokogiri'
